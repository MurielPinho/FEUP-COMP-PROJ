import A;

class Pot5 {
    /*
        Class used to demonstrate that supports overload methods.
        In this case it's func_x.
    */

    public int func_x(boolean a, boolean b) {
        return a && b;
    }

    public int func_x(int[] a, int b) {

        int x;
        x= 0-1;
        if(a.length < 2)
            x = a[0];
        else
            x=0-2;
        return x;
    }

    public int func_y(int a, int b) {
        return a - b;
    }

}